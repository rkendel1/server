{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "outDir": ".",
    "lib": ["es2020"],
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", ".vscode-test"]
}
